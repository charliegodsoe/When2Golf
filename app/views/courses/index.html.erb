<h1>Listing courses</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @courses.each do |course| %>
  <tr>
    <td><%= course.name %></td>
    <td><%= course.description %></td>
    <td><%= course.location %></td>
    <td><%= link_to 'Show', course %></td>
  </tr>
<% end %>
</table>

