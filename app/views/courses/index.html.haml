%h2 Listing courses
%table{:class => ["inner_publish", "legend"]}
	- @time_entry_qualities.each do |time_entry_quality|
		%tr
			%td{:class => time_entry_quality.name}= time_entry_quality.display_name

%table{:class => 'courses_table'}
	%thead
		%tr
			%th Name
			%th Today
			%th Today + 1
			%th Today + 2
			%th Today + 3
			%th Updated
	%tbody
		= render :partial => 'course', :collection => @courses
